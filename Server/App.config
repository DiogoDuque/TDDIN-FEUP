<?xml version="1.0" encoding="utf-8" ?>
<configuration>
  <system.runtime.remoting>
    <application name="Server">
      <service>
        <wellknown type="Server.Coordinator, Server" mode="Singleton" objectUri="Coordinator"/>
        <activated type="Register, Server"/>
      </service>
      <channels>
        <channel ref="tcp" port="5757">
          <serverProviders>
            <formatter ref="binary" typeFilterLevel="Full"/>
          </serverProviders>
        </channel>
      </channels>
    </application>
  </system.runtime.remoting>
</configuration>