<?xml version="1.0" encoding="utf-8" ?>
<configuration>
  <system.runtime.remoting>
    <application>
      <client url="tcp://localhost:5757/Server">
        <wellknown type="Server.Coordinator, Server" url="tcp://localhost:5757/Server/Coordinator.rem" />
        <activated type="Register, Server"/>
      </client>
      <channels>
        <channel ref="tcp">
          <serverProviders>
            <formatter ref="binary" typeFilterLevel="Full" />
          </serverProviders>
        </channel>
      </channels>
    </application>
  </system.runtime.remoting>
</configuration>